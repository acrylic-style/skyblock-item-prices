    <script type="text/javascript">
      function htmlDecode(input){
        const e = document.createElement('textarea');
        e.innerHTML = input;
        let bruh = ''
        e.childNodes.forEach(node => bruh += node.nodeValue)
        return bruh
      }
      const auctions = JSON.parse(htmlDecode("<%= JSON.stringify(auctions) %>"))
      const auctionsRaw = JSON.parse(htmlDecode("<%= JSON.stringify(auctionsRaw) %>"))
    </script>